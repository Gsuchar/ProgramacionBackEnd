<div class="container " >
  <h2 class="text-center my-2">Lista de Productos con Handlebars</h2>
  <div class="card-deck mb-3 text-center">
    <div class="card mb-4 box-shadow" style="width: 18rem;" >
      {{#each products}}
        <div class="card-header">  
          <h3 class="card-title">{{title}}</h3>
        </div>
        <div class="card-body">  
          <p class="card-text">Id: {{id}}</p>
          <p class="card-text">Description{{description}}</p>
          <p class="card-text">Precio: {{price}}</p>
          <p class="card-text">Codigo: {{code}}</p>
          <p class="card-text">Stock: {{stock}}</p>
          <p class="card-text">Categor√≠a: {{category}}</p>
          <p class="card-text">Thumbnail: {{thumbnail}}</p>        
        </div>
      {{/each}}
    </div>
  </div>  
</div>